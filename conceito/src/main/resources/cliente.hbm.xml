<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="br.com.mvcs.model.Cliente" table="cliente">
        <id name="id" type="int" column="id">
            <generator class="assigned"/>
        </id>
    
        <property name="razaoSocial" type="string">
            <column length="200" name="razao_social"/>
        </property>
        
        <property name="nomeFantasia" type="string">
            <column length="100" name="nome_fantasia"/>
        </property>
        
        <property name="cpfCnpj" type="string">
            <column length="20" name="cnpj_cpf"/>
        </property>
    
        <property name="contato" type="string">
            <column length="14" name="contato"/>
        </property>
        
        <property name="status" type="string">
            <column length="20" name="status_cliente"/>
        </property>
    
    </class>
</hibernate-mapping>
